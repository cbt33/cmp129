package examples.statics;

public class staticTest {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Dog fluffy = new Dog();
		Dog lassie = new Dog();
		
		fluffy.setEyes(3);
		
		System.out.println("Fluffy's eyes are " + fluffy.getEyes());
		System.out.println("Lassie's eyes are " + lassie.getEyes());
		
		lassie.setEyes(1);
		
		System.out.println("Fluffy's eyes are " + fluffy.getEyes());
		System.out.println("Lassie's eyes are " + lassie.getEyes());
		
	}

}
