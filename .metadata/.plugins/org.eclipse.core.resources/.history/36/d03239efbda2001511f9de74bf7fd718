package examples.statics;

public class Dog {
	//Called an instance variable, number of instances depends on number of objects client creates
	private int age;
	public static int eyes;
	
	public static void Calc() {
		eyes = 2;
	}
	
	public void Exec(){
		age = 44;
		eyes = 2;
	}

	public int getEyes() {
		return eyes;
	}

	public void setEyes(int eyes) {
		this.eyes = eyes;
	}
	
}
