package car_project;

import static org.junit.Assert.*;

import org.junit.Test;

public class Assignment3 {
  
  @Test
  public static void defaultConstructorTest() throws CarException {
    Car test = new Car();
    assertEquals(test, new Car());
  }
  
  @Test
  public static void overloadedConstructorTest() throws CarException {
    Car test = new Car(1971, 50000);
    assertEquals(test, new Car(1971, 50000));
  }
  
  public static void copyConstructorTest() {
    Car test = new Car(1971, 50000);
    Car copy = new Car(test);
    System.out.println(test);
  }
  public static void main(String[] args) {
    defaultConstructorTest();
    overloadedConstructorTest();
    copyConstructorTest();
  }
}
