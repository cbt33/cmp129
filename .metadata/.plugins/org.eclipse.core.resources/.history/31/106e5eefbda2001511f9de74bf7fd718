package examples.linkedlist;

public class LinkedListDemo1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Node head;
		Node tail;
		Node temp;
		head = null;
		tail = null;
		
		//Add our 1st node
		temp = new Node();
		temp.data = 5;
		temp.next = null;
		head = temp;
		tail = temp;
		
		//Add the 2nd node
		temp = new Node();
		head.next = temp;
		tail = temp;
		temp.data = 12;
		temp.next = null;
		
		//Add the 3rd node
		temp = new Node();
		head.next = temp;
		tail = temp;
		temp.data = 18;
		temp.next = null;
		
	}

}
