package examples.generics;
import examples.linkedlist.Node;

public class LinkedListDemo1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Node head;
		Node tail;
		Node temp;
		head = null;
		tail = null;

		// Add our 1st node
		temp = new Node();
		temp.data = 5;
		temp.next = null;
		head = temp;
		tail = temp;

		// Add the 2nd node
		temp = new Node();
		tail.next = temp;
		tail = temp;
		tail.data = 12;
		tail.next = null;

		// Add the 3rd node
		temp = new Node();
		tail.next = temp;
		tail = temp;
		tail.data = 18;
		tail.next = null;

	}

}
