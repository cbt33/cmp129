package car_project;

public class carCountTest {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		boolean end = false;
		while (!end) {
			Car car1 = new Car();
			System.out.println(car1.getCarCount());
			Car car2 = new Car();
			System.out.println(car1.getCarCount());
			Car car3 = new Car();
			System.out.println(car1.getCarCount());
			car1 = null;
			car2 = null;
			end = true;
		}
		System.runFinalization();
		System.gc();
		System.out.println(car3.getCarCount());
	}

}
